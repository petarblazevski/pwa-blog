{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "techiediaries.com",
      "urls": ["https://www.techiediaries.com/**"],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    }
  ]
}
